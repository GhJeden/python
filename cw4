mark = int(input('Enter your grade for your work: '))

if mark >= 90 and mark <= 100:
    print('Excellent')
elif mark >= 70 and mark <= 89:
    print('Good') 
elif mark >= 50 and mark <= 69:
    print('It`s okay')
elif mark <= 50:
    print('Bad')
else:
    print("It`s not a grade from 1 to 100")


work = int(input('Enter your work years: '))
salary = int(input('Enter your salary sum: '))

if work < 1:
    print("You can`t have a bigger premie")
elif work >= 1 and work <= 3:
    print(f'(salary / 100 * 5) + salary = {(salary / 100 * 5) + salary}')
    print("There your premie")
elif work >= 3 and work <= 5:
    print(f'(salary / 100 * 10) + salary = {(salary / 100 * 10) + salary}')
    print('There your premie')
elif work > 5:
    print(f'(salary / 100 * 15) + salary = {(salary / 100 * 15) + salary}')
    print("There ur premie")

fDN = int(input('Enter your 4-digit number: '))
    
result1 = fDN // 1000
result2 = (fDN // 100) % 10
result3 = (fDN // 10) % 10
result4 = fDN % 10

result5 = result1 + result2 + result3 + result4

if result5 % 2 == 0:
    print("The number is pair")
else:
    print("The number isn`t pair")

