us_num = int(input("enter your number already ^_^ : "))


if us_num < 100000 or us_num > 999999:
    print("я твоей маме позвоню скажу что ты матешу прогуливал")
else:

    peace1 = us_num // 100000
    peace2 = (us_num // 10000) % 10
    peace3 = (us_num // 1000) % 10
    peace4 = (us_num // 100) % 10
    peace5 = (us_num // 10) % 10
    peace6 = us_num % 10

    fin_num = ( peace6 * 100000 + peace5 * 10000 + peace3 * 1000 + peace4 * 100 + peace2 * 10 + peace1
    )

    print("Ответ:", fin_num)